{
  "name": "@apps/social-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "typeorm": "ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli -d src/datasource",
    "migration:generate": "npm run typeorm -- migration:generate src/migrations/social -p",
    "migration:create": "cross-var ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli migration:create src/database/migrations/social",
    "migration:run": "npm run typeorm -- migration:run",
    "test:migration:run": "ENV=test npm run typeorm -- migration:run",
    "test:e2e": "ENV=test jest --config ./test/jest-e2e.json --maxWorkers=1 --detectOpenHandles --testTimeout=15000",
    "test": "ENV=test jest --maxWorkers=1 --detectOpenHandles"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "29.2.4",
    "cross-var": "^1.1.0",
    "jest": "29.3.1",
    "ts-jest": "29.0.3",
    "ts-node": "^10.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^libs/(.*)": "<rootDir>../../libs/$1",
      "@app/infra": "<rootDir>../../libs/infra/src/index",
      "^apps/(.*)": "<rootDir>../../apps/$1",
      "@libs/infra/(.*)": "<rootDir>/../../libs/infra/src/$1",
      "@libs/common": "<rootDir>/../../libs/common/src"
    }
  }
}
